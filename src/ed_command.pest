

current = { "." }

last = { "$" }

absolute = { ASCII_DIGIT+ }

address = { current | last | absolute }

range_separator = { ";" | "," }

range = ${
    address* ~ range_separator* ~ address*
}

command = { "wq" | "w" | "q" | "p" | "i" }

arg = { ANY+ }

line = { SOI ~ range ~ command* ~ arg* ~ EOI }
